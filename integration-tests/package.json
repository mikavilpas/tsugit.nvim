    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-oxlint": "1.28.0",
    "oxlint": "1.28.0",
    "oxlint-tsgolint": "0.7.0",
    "tsx": "4.20.6",
    "type-fest": "5.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
