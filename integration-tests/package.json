{
  "name": "@tsugit.nvim/integration-tests",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "cy:open": "cypress open --e2e",
    "cy:run": "concurrently --success command-cypress --kill-others --names 'app,cypress' --prefix-colors 'blue,yellow' 'pnpm tui start' 'pnpm dev:server' 'wait-on --timeout 60000 http://127.0.0.1:3000 && npx cypress run'",
    "dev": "concurrently --kill-others --names 'app,cypress' --prefix-colors 'blue,yellow' 'pnpm tui start' 'pnpm cy:open'",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --ext '*' --exec 'npx tsx ./server/server.ts'",
    "eslint": "eslint --max-warnings=0 ."
  },
  "devDependencies": {
    "@tui-sandbox/library": "4.0.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "concurrently": "9.1.0",
    "cypress": "13.15.2",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "wait-on": "8.0.1",
    "zod": "3.23.8"
  }
}
