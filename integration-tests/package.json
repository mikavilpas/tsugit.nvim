    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-oxlint": "1.43.0",
    "oxlint": "1.43.0",
    "oxlint-tsgolint": "0.11.4",
    "tsx": "4.21.0",
    "type-fest": "5.4.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
